import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int[] arr={1,2,3,4,5};
		int element=40;
		int[] newarray=new int[arr.length];
		for(int i=0;i<arr.length;i++){
    	newarray[i]=arr[i];
	   System.out.print(newarray[i]);
		}
		newarray[1]=element;
		for(int i=0;i<newarray.length;i++)
		{
		    System.out.println("The array is " + newarray[i]);
		}
	}
}
